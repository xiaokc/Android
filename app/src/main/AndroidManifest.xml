<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.xkc.android" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>

        <receiver android:name=".MyReveiver">
            <intent-filter>
                <action android:name="senz.intent.action.HOME_OFFICE_STATUS"/>
                <action android:name="senz.intent.action.MOTION_CHANGED"/>
                <action android:name="senz.intent.action.EVENT"/>

            </intent-filter>
        </receiver>


        <activity android:name=".RichEditorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
